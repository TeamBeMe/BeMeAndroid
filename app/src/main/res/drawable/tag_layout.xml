<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start="2dp"
        android:end="2dp"
        android:top="2dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp"
                android:color="#DFDFDF"/>
            <solid android:color="@color/white"/>
            <corners
                android:radius="6dp"/>
        </shape>
    </item>
</layer-list>